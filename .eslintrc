{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
  },
  "globals": {
    "chrome": false,
    "VERSION": true
  },
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-use-before-define": 0,
    "react/jsx-no-bind": 0
  },
  "plugins": [
    "react"
  ]
}
